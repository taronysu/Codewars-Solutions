// Sum Numbers
function sum (numbers) {
  let s = 0;
for(let i=0; i<numbers.length; i++) {
  s += numbers[i];
  
}    
    return s;
}


// Another Solution

function sum (numbers) {
    return numbers.reduce((el,val)=>el+val,0);      
}
