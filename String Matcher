function isMatching(string, str1, str2) {
  let s = string.toLowerCase().replace(/\s+/g, '').split('').sort().join('');
  let strs =  (str1+str2).toLowerCase().replace(/\s+/g, '').split('').sort().join('');
      return s === strs;
  }
