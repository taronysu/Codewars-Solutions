https://www.codewars.com/kata/58e93b4706db4d24ee000096/train/javascript

function daysRepresented(trips){
   let obj = {};
   let res = 0;
   for(let el of trips){
     for(let i = el[0]; i <= el[1]; i++){
       if(obj[i]) obj[i]++;
         else {obj[i] = 1; 
               res++;
         }
      } 
   }
  return res;
 }
