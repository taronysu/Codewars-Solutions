https://www.codewars.com/kata/5b39e3772ae7545f650000fc/train/javascript

function removeDuplicateWords (s) {
    let arr = []
    s = s.split(' ')
    for (let i = 0; i < s.length; i++) {
      if(arr.includes(s[i])) {
        continue;
      }
      else {
        arr.push(s[i]);
      }
}
  return arr.join(' ');
  }
